{
  "type": "object",
  "properties": {
    "Reports": {
      "type": "array",
      "selectorAll": "table.gridview > tbody tr",
      "items": {
        "type": "object",
        "properties": {
          "Number": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 1
          },
          "URL": {
            "type": "string",
            "selector": "table.gridview > tbody tr td a",
            "control": "property.href"
          },
          "Weeks": {
            "type": "string",
            "control": "property.innerText",
            "selector": "table.gridview > tbody tr td",
            "selectIndex": 3
          },
          "Area": {
            "type": "string",
            "selector": "table.gridview > tbody tr td",
            "control": "property.innerText",
            "selectIndex": 7
          }
        }
      }
    },
    "IsLanding": {
      "type": "identifier",
      "selector": "html",
      "control": "url.Reports/ViewReports.aspx"
    }
  },
  "connect": [
    {
      "match": [
        "IsLanding",
        "IsLanding"
      ],
      "screens": [
        "reportslist"
      ]
    }
  ]
}
